Q Table 1
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i=1,x;
    scanf("%d",&n);
    scanf("%d",&x);
    while(i<=10)
    {
        printf("%d\n",n*i);
        i++;
    }
    return 0;
}
Q Odd numbers 3
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        if(i%2!=0)
        printf("%d ",i);
    }
    return 0;
}
Q Sum of n Natural Numbers
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,sum=0;
    scanf("%d",&n);
    for(int i=n;i>=1;i--)
    {
        sum=sum+i;
    }
    printf("%d",sum);
    return 0;
}
Q Leap years 2
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        if((i%4==0)&&((i%100!=0)||(i%400==0)))
            printf("%d ",i);
    }
    return 0;
}
Q Sum Digits 1
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,a=0,b=0;
    scanf("%d",&n);
    while(n>0)
    {
        a=n%10;
        b=b+a;
        n=n/10;
    }
    printf("%d",b);
    return 0;
}
Q Reverse the number
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,a=0,b=0;
    scanf("%d",&n);
    while(n>0)
    {
        a=n%10;
        b=a+(b*10);
        n=n/10;
    }
    printf("%d",b);
    return 0;
}
Q Palindrome number 2
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,a=0,b=0,c=0;
    scanf("%d",&n);
    c=n;
    while(n>0)
    {
        a=n%10;
        b=a+(b*10);
        n=n/10;
    }
    if(b==c)
        printf("Palindrome.");
    else
        printf("Not Palindrome.");
    return 0;
}
Q Check power
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int n,a;
    scanf("%d",&n);
    a= 1;
    for (int i=0;i<n;i++)
    {
        if (a==n){
            printf("1");
            break;
        }
        a*=2;
    }
    if (a!=n){
        printf("0");
    }
    return 0;
}
Q Sum of Digits 38
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,n,s=0,m,i;
    scanf("%d",&a);
    for (i = 1;i<=a;i++){
        scanf("%d",&n);
        while(n>0){
            m = n%10;
            s = s+m;
            n = n/10;
        }
        printf ("%d\n",s);
        s = 0;
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
