Q natural number series
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,s=0;
    scanf("%d",&a);
    for (int i=0;i<=a;i++){
        s = s+i;
    }
    printf("%d",s);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
